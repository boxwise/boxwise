    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-firestore": "^1.0.0",
    "react-ga": "^2.5.7",
    "react-hook-form": "^2.1.17",
    "react-intl": "^2.8.0",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-polyfill": "^6.26.0",
    "colors": "^1.3.3",
    "cypress": "^3.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "expect-firestore": "^1.0.1",
    "firebase-mock": "^2.2.10",
    "firebase-tools": "^6.6.0",
    "husky": "^1.3.1",
    "jest": "23.6.0",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^8.1.5",
    "minimist": "^1.2.0",
    "prettier": "^1.17.0",
    "prompt": "^1.0.0",
    "yarnhook": "^0.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --modulePaths=src",
    "eject": "react-scripts eject",
    "prettier": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
    "deploy-firestore": "firebase deploy --only firestore",
    "setup": "node utils/generate-env.js",
    "add-test-data": "node utils/generate-test-data.js",
    "cypress:open": "cypress open"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.js": [
      "eslint --fix --max-warnings 0",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "post-checkout": "node checkdeploy",
      "post-merge": "node checkdeploy",
      "post-rewrite": "node checkdeploy",
      "pre-commit": "lint-staged"
    }
  }
}
