    "react-ga": "^2.5.3",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "typeface-roboto": "^0.0.54",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
