    "firebase-tools": "^3.18.5",
    "husky": "^0.14.3",
    "jest-enzyme": "^6.0.1",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.4",
    "yarnhook": "^0.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,json,css}\"",
